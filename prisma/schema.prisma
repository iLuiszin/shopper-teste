generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Measure {
  measure_uuid      String  @id @default(uuid())
  measure_value     Int
  customer_code     String
  measure_type      String
  measure_datetime  DateTime
  image_url         String
  has_confirmed     Boolean  @default(false)
}