generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Reading {
  measure_uuid      String  @id @default(uuid())
  measure_value     Int
  customer_code     String
  measure_type      String
  measure_datetime  DateTime
  confirmed         Boolean  @default(false)
}